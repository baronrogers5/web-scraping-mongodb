 if(action.match(/^Pos/) && ga_clk == 0){
  ga_clk = ga_clk + 1;var firstClick = 'FirstClick-'+posno[1]; var filterArr = new Array(); var filter = '';
  if(city_search) filterArr.push('L');
  if(filterArr.length > 0) filter = filterArr.join("");
  if(filter){filter = filter+'F';firstClick += '-'+filter;}
  if(ims.bizFilterAppeared == 1) firstClick += '-BFilter';
  imgtm.push({ 'event' : 'IMEvent','eventCategory' : category,'eventAction' : firstClick,'eventLabel' : linkHref,'eventValue': value});
 }
 if(!nonInteractionFlag) nonInteractionFlag = false;
if(category == 'EXP'){
    imgtm.push({ 'event' : 'optimize.activate','eventCategory' : category,'eventAction' : action,'eventLabel' : linkHref,'eventValue': value});
}else{
