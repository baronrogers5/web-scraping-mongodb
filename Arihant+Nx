    ims.loadScriptCalled=0;
    function loadScript() {
        ims.loadScriptCalled++;
        if(ims.loadScriptCalled > 3){
            return;
        }
        var slash = "/";
        var u = "utils.imimg.com"+slash;
        var a = "apps.imimg.com"+slash;
        if(location.hostname.match(/dev/)){
            u = 'dev-utils.imimg.com'+slash;
            a = "dev-apps.imimg.com"+slash;
        }else if(location.hostname.match(/stg/)){
            u = 'stg-utils.imimg.com'+slash;
            a = "stg-apps.imimg.com"+slash;
        }
        var p='imsrchui'+slash+'js'+slash;
        var hp='globalhf'+slash+'header'+slash;

        var jquery  = u+p+'jquery.js';
        var pre     = u+p+'pre.min-v269.js';
        var blEnq   = a+'js'+slash+'reqForms.min.js?v=68';
        var jq_ac   = u+'suggest'+slash+'js'+slash+'suggest.js';
        var hdr     = u+hp+'header-new120.js';
        var links = [jquery, hdr, jq_ac, pre, blEnq];
        if(ims.loadScriptCalled===1){
            loadGtm();
            links = [jquery];
        }else if(ims.loadScriptCalled===2){
            links = [ hdr, pre, jq_ac];
        }else if(ims.loadScriptCalled===3){
            links = [ blEnq];
        }

        var headElement = document.getElementsByTagName("head")[0],
            script, i;
        for (i = 0; i < links.length; i++) {
            script = document.createElement("script");
            script.setAttribute('type', 'text/javascript');
            script.setAttribute('crossorigin', 'anonymous');
            if (script.readyState) { //IE
                script.onreadystatechange = function() {
                    if (script.readyState == "loaded" ||
                        script.readyState == "complete") {
                        script.onreadystatechange = null;
                    }
                };
            }
            script.src = '//'+links[i];
            headElement.appendChild(script);
            if(i===0 && ims.loadScriptCalled===1){
                triggerJqueryDefined('jQuery', 5, 50);
            var interval = setInterval(function () {
                   if ( typeof(jQuery) != 'undefined' && typeof(jQuery.isReady) === 'boolean' && jQuery.isReady ) {
                       clearInterval(interval);
                       //$(document).fire('_jquery_ready');

                   }
            },10);
            }
        }
    }
    if (window.addEventListener)
        window.addEventListener("load", loadScript, false);
    else if (window.attachEvent)
        window.attachEvent("onload", loadScript);
    else window.onload = loadScript;


document.addEventListener('preready', function (e) {
